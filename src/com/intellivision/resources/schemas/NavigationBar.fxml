<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!--
    Document   : NavigationBar.fxml
    Created on : Oct 20, 2012, 01:00:00 PM
    Author     : Andrey Pudov        <mail@andreypudov.com>
    Description:
        Navigation bar with back and next buttons to going between modules.
-->

<fx:root fx:id="moduleBar" type="javafx.scene.layout.Region"
         prefWidth="57.0"  minWidth="57.0"  maxWidth="57.0"
         prefHeight="22.0" minHeight="22.0" maxHeight="22.0"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="com.intellivision.ui.controllers.NavigationBarController">
    <children>
        <Button fx:id="navigationBarBack"
                minWidth="28.0" maxHeight="1.7976931348623157E308"
                onAction="#navigationBarNextOnAction" />
        <Button fx:id="navigationBarNext"
                minWidth="28.0" maxHeight="1.7976931348623157E308"
                onAction="#navigationBarNextOnAction" />
    </children>
    <stylesheets>
        <URL value="@../styles/NavigationBar.css" />
    </stylesheets>
</fx:root>
